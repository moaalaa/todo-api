{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "todo app api",
  "scripts": {
    "watch": "nodemon ./server/server.js",
    "postinstall": "link-module-alias",
    "preinstall": "link-module-alias clean"
  },
  "main": "app.js",
  "license": "MIT",
  "_moduleAliases": {
    "~root": ".",
