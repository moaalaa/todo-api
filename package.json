  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "express-group-routes": "^1.1.0",
    "global": "^4.3.2",
    "mongodb": "^3.2.3",
    "mongoose": "^5.5.3",
    "nodemon": "^1.19.0"
  },
  "devDependencies": {
    "link-module-alias": "^1.2.0"
  }
}
